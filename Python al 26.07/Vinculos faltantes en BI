import pandas as pd

df_left = pd.read_csv(r'E:\Usuarios\fgonzalez\OneDrive - bcp.gov.py\Escritorio\Vane\Fusion_1046\19 tanda\Vinculos_Ueno_BI.csv', sep=';')
df_right = pd.read_csv(r'E:\Usuarios\fgonzalez\OneDrive - bcp.gov.py\Escritorio\Vane\Fusion_1046\19 tanda\UenoVinculosModif.csv', sep=';')

merged = df_left.merge(df_right, on='Llave Concatenada', how='left', suffixes=('_modif', '_original'), indicator=True)
only_in_left = merged[merged['_merge'] == 'left_only']

pd.set_option('display.max_columns', None)
print(only_in_left)

only_in_left.to_csv(r'E:\Usuarios\fgonzalez\OneDrive - bcp.gov.py\Escritorio\Vane\Fusion_1046\19 tanda\Vinculos_Solo_BI.csv', index=False, sep=';')
